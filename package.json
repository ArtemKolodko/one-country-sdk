{
  "name": "one-country-sdk",
  "version": "0.3.1",
  "description": "Package allows easily interact with 1.country domains",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "test": "jest",
    "prepack": "npm run build"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArtemKolodko/one-country-sdk.git"
  },
  "keywords": [
    "npm",
    "javascript",
    "typescript",
    "esm",
    "cjs",
    "nodejs",
    "commonjs",
    "ecmascript",
    "beginner",
    "example",
    "demonstration"
  ],
  "author": "Artem Kolodko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArtemKolodko/one-country-sdk/issues"
  },
  "homepage": "https://github.com/ArtemKolodko/one-country-sdk/issues#readme",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "bn.js": "^5.2.1",
    "dotenv": "^16.0.3",
    "web3": "^1.8.1"
  }
}
